{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "cache-first",
      "urls": [
        "//api.giphy.com/v1/gifs/"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "24h",
        "timeout": "60s",
        "strategy": "performance"
      }
    }
  ]
}
